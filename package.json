{
  "name": "@equinor/videx-storybook-input",
  "version": "1.0.0",
  "description": "User input modules for storybook.",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "browser": "dist/index.umd.js",
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "scripts": {
    "storybook": "start-storybook",
    "prebuild": "rimraf dist",
    "build": "rollup -c",
    "prepub": "npm run build",
    "pub": "npm publish --access=public",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "predocs": "rimraf docs",
    "docs": "typedoc --out docs src",
    "postdocs": "copyfiles images/* docs && copyfiles .nojekyll docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/equinor/videx-storybook-input.git"
  },
  "keywords": [
    "videx",
    "input",
    "typescript",
    "storybook"
  ],
  "author": "Tom Kristian Tjemsland",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/equinor/videx-storybook-input/issues"
  },
  "homepage": "https://github.com/equinor/videx-storybook-input#readme",
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@storybook/html": "^5.2.6",
    "@types/d3": "^5.7.2",
    "@types/jest": "^24.0.21",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "copyfiles": "^2.1.1",
    "d3": "^5.12.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "jest": "^24.9.0",
    "rimraf": "^3.0.0",
    "rollup": "^1.26.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.1.2",
    "rollup-plugin-typescript2": "^0.24.3",
    "ts-jest": "^24.1.0",
    "typedoc": "^0.15.0",
    "typescript": "^3.6.4"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "/test/.*",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
}
